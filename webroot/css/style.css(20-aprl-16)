[type="checkbox"]:not(:checked),
[type="checkbox"]:checked,[type="radio"]:not(:checked),[type="radio"]:checked {
  position: absolute;
  left: -9999px;
}

[type="checkbox"]:not(:checked) + label, 
[type="checkbox"]:checked + label,[type="radio"]:not(:checked) + label,[type="radio"]:checked + label{
  position: relative;
  cursor: pointer;
}

[type="checkbox"]:not(:checked) + label:before,
[type="checkbox"]:checked + label:before {
  content: '';
  position: absolute;
  top: -2px; left: 0px;
   width:20px;
  height: 20px;
  border: 2px solid #056a1f;
  border-radius:4px;

}
[type="checkbox"]:not(:checked) + label:after,
[type="checkbox"]:checked + label:after {
  content: '';
  position: absolute;
  top:-6px; left: 4px;
  width: 20px;
  height:19px;
  background: url(../images/tick.png) no-repeat;
  color: #056a1f;
  transition: all .2s;
}


[type="radio"]:not(:checked) + label:before,[type="radio"]:checked + label:before{
  content: '';
  position: absolute;
  top: -2px; left: 0px;
  width:20px;
  height: 20px;
  border: 2px solid #056a1f;
  border-radius:100%;

}

/* checkbox aspect */
/* checked mark aspect */


[type="radio"]:not(:checked) + label:after,
[type="radio"]:checked + label:after {
  content: '';
  position: absolute;
  top:3px; left: 5px;
  width: 10px;
  height:10px;
  border-radius:100%;
  background: #056a1f;
  transition: all .2s;
}

/* checked mark aspect changes */
[type="checkbox"]:not(:checked) + label:after,[type="radio"]:not(:checked) + label:after {
  opacity: 0;
  transform: scale(0);
}

[type="checkbox"]:checked + label:after,[type="radio"]:checked + label:after {
  opacity: 1;
  transform: scale(1);
}
